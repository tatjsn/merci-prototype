{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "rules": {
    "strict": [
      0
    ],
    "arrow-parens": [
      2,
      "as-needed"
    ],
    "no-mixed-operators": [
      0
    ],
    "no-plusplus": [
      0
    ],
    "class-methods-use-this": [
      0
    ],
    "import/prefer-default-export": [
      0
    ]
  },
  "globals": {
    "jest": true,
    "jasmine": true,
    "describe": true,
    "it": true,
    "expect": true,
    "beforeEach": true,
    "afterEach": true,
    "spyOn": true,
    "fail": true
  }
}